@model string
@{
    ViewData["Title"] = "Perimeter";
}
<html>
<br />
<head>
    <link rel="stylesheet" href="~/css/Perimeter.css"/>
    <!--montserrat-->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=DM+Sans:wght@700&family=Montserrat:wght@400;500;700&display=swap" rel="stylesheet">
</head>
<body>
    <br />
    <div>@Html.Raw(Model)</div>
    @if (!string.IsNullOrEmpty(ViewData["pr"] as string))
    {
        <pre class="rectangle">@Html.Raw(ViewData["pr"] as string)</pre>
    }
</body>
</html>
